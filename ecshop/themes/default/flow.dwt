function checkIpt(item){
  var val = item.value;
  var method = document.getElementById('payMethod');
  if(val == 'wxpay'){
    method.innerHTML = '微&nbsp;&nbsp;&nbsp;信';
  }else{
    method.innerHTML = '支付宝';
  }
  document.getElementById('yunqi_payment').click();
}

$(function(){
  if($(".user-list ul li").length>0){
    $(".user-list ul li").click(function(){
      $(this).addClass("active").siblings().removeClass("active");
      if($(this).hasClass("insured")){
        $("#recognizee_name").val($(this).attr("data-username"));//姓名
        $("#recognizee_cardtype").val($(this).attr("data-card-type"));//证件类型
        $("#recognizee_cardnum").val($(this).attr("data-card-num"));//证件号码
        $("#recognizee_birth").val($(this).attr("data-birth-date"));//出生日期
        //$("input[name='recognizee_sex']").eq($(this).attr("data-sex")).prop("checked", true);//性别
        $("#recognizee_sex").val($(this).attr("data-sex"));
        $("#recognizee_phone").val($(this).attr("data-tel"));//手机号码
      }else if($(this).hasClass("holder")){
        $("#policy_name").val($(this).attr("data-username"));
        $("#policy_cardtype").val($(this).attr("data-card-type"));
        $("#policy_cardnum").val($(this).attr("data-card-num"));
        $("#policy_birth").val($(this).attr("data-birth-date"));
        //$("input[name='policy_sex']").eq($(this).attr("data-sex")).prop("checked", true);
        $("#policy_sex").val($(this).attr("data-sex"));
        $("#policy_phone").val($(this).attr("data-tel"));
      }else{}
    })
  }
})
</script>
